//preprocessor directive - scanf(),printf()
#include<stdio.h>
int main (){
    int a;
    int b;
int candelas [3][4] = {{32,27,46,59},{54,53,44,35},{34,67,53,24}};
    for(a=0;a<3;a++){
    for(b=0;b<4;b++){
    printf("Candelas[%d][%d] = %d\n",a,b,candelas[a][b]);
    }
    }
    return 0;
    }
