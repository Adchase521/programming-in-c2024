//preprocessor directive - scanf() , printf()
#include<stdio.h>
int main (){
    int start ,sum,stop;
    printf("Enter the value for when to start:");
    scanf("%d,",&start);
    printf("Enter the value for when to stop:");
    scanf("%d,",&stop);
    do{
    printf("%d\n",start);
        start++;
        sum +=start;
        
      
     }while (start<=stop);   
     
     printf("The sum is %d\n",sum);
     return 0;
}        
