//preprocessor directive - scanf(),printf().
#include<stdio.h>
int main (){
    int a,b,c;
    
int volts[3][2][4] = { {{34,56,23,89},{23,87,12,94}},{{34,56,23,89},{23,87,12,94}},{{34,56,23,89},{23,87,12,94}} };
    for(a=0;a<3;a++){
    for(b=0;b<2;b++){
    for(c=0;c<4;c++)
    
    printf("Volts[%d][%d][%d]=%d\n",a,b,c,volts[a][b][c]);
    }
    }
    
    return 0;
    
    }
